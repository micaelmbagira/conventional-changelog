{
  "name": "@release-it/conventional-changelog",
  "version": "8.0.1",
  "description": "Conventional changelog plugin for release-it",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "bron test.js --serial",
    "release": "release-it"
  },
  "keywords": [
    "release",
    "release-it",
    "release-it-plugin",
    "plugin",
    "conventional",
    "changelog",
    "recommended",
    "bump"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/release-it/conventional-changelog.git"
  },
  "homepage": "https://github.com/release-it/conventional-changelog#readme",
  "bugs": "https://github.com/release-it/conventional-changelog/issues",
  "author": {
    "name": "Lars Kappert",
    "email": "lars@webpro.nl",
    "url": "https://webpro.nl"
  },
  "dependencies": {
    "concat-stream": "^2.0.0",
    "conventional-changelog": "^5.1.0",
    "conventional-recommended-bump": "^9.0.0",
    "semver": "^7.6.2",
    "git-semver-tags": "^7.0.0"
  },
  "devDependencies": {
    "bron": "^2.0.3",
    "installed-check": "^9.3.0",
    "release-it": "^17.5.0",
    "shelljs": "^0.8.5",
    "sinon": "^18.0.0",
    "tmp": "^0.2.3"
  },
  "peerDependencies": {
    "release-it": "^17.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "release-it": {
    "hooks": {
      "before:init": "npm test && installed-check"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_TOKEN_RELEASE_IT"
    }
  }
}
